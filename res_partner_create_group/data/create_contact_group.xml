<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="group_create_contact" model="res.groups">
        <field name="name">Create Contact</field>
        <field name="category_id" ref="base.module_category_hidden" />
    </record>

    <record id="access_create_contact" model="ir.model.access">
        <field name="name">res_partner create_contact_group</field>
        <field
      name="model_id"
      search="[('model','=','res.partner')]"
      model="ir.model"
    />
        <field name="group_id" ref="res_partner_create_group.group_create_contact" />
        <field name="perm_read">True</field>
        <field name="perm_write">True</field>
        <field name="perm_create">True</field>
        <field name="perm_unlink">True</field>
    </record>

    <data noupdate="1">
        <function model="ir.model.access" name="init" />
    </data>

</odoo>
