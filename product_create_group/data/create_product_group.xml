<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="group_create_product_tmpl" model="res.groups">
        <field name="name">Create Product Tmpl</field>
        <field name="category_id" ref="base.module_category_hidden" />
    </record>

    <record id="access_create_product_tmpl" model="ir.model.access">
        <field name="name">product_template create_product_tmpl_group</field>
        <field
      name="model_id"
      search="[('model','=','product.template')]"
      model="ir.model"
    />
        <field name="group_id" ref="product_create_group.group_create_product_tmpl" />
        <field name="perm_read">True</field>
        <field name="perm_write">True</field>
        <field name="perm_create">True</field>
        <field name="perm_unlink">True</field>
    </record>

    <data noupdate="1">
        <function model="ir.model.access" name="init" />
    </data>

</odoo>
